<% url = OsPhoneBook::Application.routes.url_helpers %>

$(document).ready(function() {
  if(document.location.pathname != "<%= url.root_path %>") {
    $(document).bind('keypress', 's', function() {
      window.location.href = '<%= url.root_path %>';
    });
  }
});
